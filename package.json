{
  "name": "eslint-traverser",
  "version": "1.0.1",
  "description": "A utility that helps traverse code the way ESLint does",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "unit": "istanbul cover jasmine JASMINE_CONFIG_PATH=test/jasmine.json",
    "test": "npm run lint  && npm run coveralls",
    "coveralls": "npm run unit && cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganimomer/eslint-traverser.git"
  },
  "keywords": [
    "eslint",
    "utility",
    "traverse",
    "ast",
    "estree",
    "esparse"
  ],
  "author": "Omer Ganim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ganimomer/eslint-traverser/issues"
  },
  "homepage": "https://github.com/ganimomer/eslint-traverser#readme",
  "devDependencies": {
    "coveralls": "2.11.14",
    "eslint-config-wix-editor": "0.2.3",
    "istanbul": "0.4.5",
    "jasmine": "2.4.1",
    "mocha": "3.0.2"
  },
  "dependencies": {
    "eslint": "^3.5.0",
    "lodash": "^4.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
